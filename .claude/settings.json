{
  "alwaysThinkingEnabled": false,
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Glass.aiff"
          }
        ]
      }
    ]
  },
  "permissions": {
    "allow": [
      "Bash(chmod:*)",
      "Bash(gemini:*)",
      "Bash(docker logs:*)",
      "Bash(docker exec:*)",
      "Bash(docker-compose stop:*)",
      "Bash(docker rm:*)",
      "Bash(docker volume rm:*)"
    ],
    "deny": [],
    "ask": []
  }
}
